    "buffers": "^0.1.1",
    "debug": "^2.2.0",
    "q": "^1.1.2"
  },
  "devDependencies": {
    "istanbul": "^0.3.19",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "jshint-full-path": "^1.1.1",
    "mocha": "^2.3.0",
    "should": "^7.1.0"
